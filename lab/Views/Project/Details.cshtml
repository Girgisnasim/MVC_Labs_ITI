@{
}
@model Project


<div class="card">
    <div class="card-body">
        <h5 class="card-title">Project Details</h5>
        <h6 class="card-subtitle mb-2 text-muted">@Model.Pname</h6>
        <p class="card-text">@Model.Plocation</p>
        <p class="card-text">@Model.City</p>
        <p class="card-text">@Model.DeptProject.Dname</p>
        <a href="/project/index" class="card-link">Back To List</a>
    </div>
</div>


@section Slide_title {
    @Model.Pname 
}